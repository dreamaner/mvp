<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="com.android.mvp">

    <application android:allowBackup="true"
        android:name=".AppInit"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <service android:name="com.android.kit.utils.network.NetworkService"/>
        <receiver android:name=".net.NetReceiver">
            <intent-filter>
                <action android:name="com.network.state.action"/>
            </intent-filter>
        </receiver>
    </application>

</manifest>
